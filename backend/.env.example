# Backend ENV example for Render/local

# Server
PORT=3000
DB_PATH=./data/app.db
CORS_ORIGINS=

# Direct buy URL (Transak)
# Example: https://global.transak.com?apiKey=YOUR_KEY&network=bsc&cryptoCurrencyCode=USDT
TRANSAK_BUY_URL=

# 1inch Proxy
ONEINCH_API_KEY=your_1inch_api_key
ONEINCH_UPSTREAM_BASE=https://api.1inch.dev

# Moralis Proxy
MORALIS_API_KEY=your_moralis_api_key
MORALIS_UPSTREAM_BASE=https://deep-index.moralis.io/api/v2.2

# App environment
APP_ENV=dev
